public class WorkShift{
    public static void main(String[] args){
        int h = 20;
        int m = 42;
        int s = 16;
        
        h *= 3600;
        m *= 60;
        
        System.out.println("  " + h);
        System.out.println("  " + m);
        System.out.println("+ " + s);
        System.out.println("--------");
        System.out.println(h + m + s);
    }
}
